{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Hero Coaches Overlay",
    "version": "0.1.0",
    "minimum-overwolf-version": "0.169.0",
    "author": "Hero Coaches",
    "icon": "icon.png",
    "icon_gray": "icon.png",
    "description": "游戏复盘分析 Overlay"
  },
  "permissions": [
    "GameInfo",
    "Profile"
  ],
  "data": {
    "game_targeting": {
      "type": "dedicated",
      "game_ids": []
    },
    "start_window": "postmatch",
    "windows": {
      "postmatch": {
        "file": "dist/postmatch.html",
        "transparent": false,
        "resizable": true,
        "size": {
          "width": 450,
          "height": 300
        },
        "min_size": {
          "width": 400,
          "height": 250
        },
        "max_size": {
          "width": 600,
          "height": 400
        }
      }
    },
    "game_events": [
      {
        "name": "match_info",
        "filters": [
          {
            "key": "match_outcome",
            "value": "victory,defeat,draw"
          }
        ]
      }
    ]
  }
}

